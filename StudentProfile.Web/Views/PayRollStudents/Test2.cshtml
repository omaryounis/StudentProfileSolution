
@{
    ViewBag.Title = "اصدار الشبكات";
    Layout = "~/Views/Shared/_DevextremeLayout.cshtml";
}

<div class="container-fluid container-fullw ng-scope">
    <div class="col-md-12">

        <div class="panel bg-white">
            <div class="breadcrumb-wrapper text-right">
                <h4 class="no-margin"> اصدار الشبكات </h4>
            </div>
            <div class="panel-body text-right">
                <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12">
                    <label class="control-label"> مجموعة اوامر الصرف</label>
                    <div dx-tag-box="simple"></div>
                </div>
                <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12">
                    <label class="control-label"> ارقام الشبكات</label>
                    <div dx-text-box="textBox.simple"></div>
                </div>
                <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12">
                    <label class="control-label"> القيمة المالية</label>
                    <div dx-text-box="textBox.simple"></div>
                </div>
                <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12">
                    <label class="label-control col-lg-12 col-md-12 col-sm-12 col-xs-12 mb-12"><br /></label>
                    <div dx-button="{
                                stylingMode: 'text',
                                text: 'حفظ',
                                type: 'success',
                                width: 120,
                                onClick: showMessage
                            }"></div>
                </div>
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 dx-tree-st">
                    <div dx-data-grid="simple"> </div>
                </div>
            </div>
        </div>
    </div>
</div>



