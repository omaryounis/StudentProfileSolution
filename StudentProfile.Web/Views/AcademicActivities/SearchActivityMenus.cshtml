@{
    ViewBag.Title = "بحث قوائم مشاركات الطلاب";
    Layout = "~/Views/Shared/_DevextremeLayout.cshtml";
}

<script src="~/Scripts/AcademicActivities/SearchActivityMenusController.js"></script>
@*<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>*@
<style>
    .swal-text {
        text-align: center;
    }
</style>
<div class="container-fluid container-fullw">
    <div ng-controller="SearchActivityMenusCtrl">

        <div class="row">
            <div class="col-md-12">
                <div class="panel panel-default">
                    <div class="breadcrumb-wrapper text-right">
                        <h2>بحث قوائم مشاركات الطلاب</h2>
                    </div>
                    <div class="panel-body">
                        <div class="panel panel-default">
                            <div class="panel-body">
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                        @* ActivityMainGrid *@
                                        <div class="dx-tree-st dx-tree-st22" dx-data-grid="ActivityMainGrid" dx-item-alias="ActivityMainItem">

                                            <div data-options="dxTemplate: { name : 'ActivityRequestMasterContent' }">

                                                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 dx-tree-st-new">

                                                    @*<div class="master-detail-caption-st"></div>*@
                                                    <div dx-data-grid="GetActivityRequestMaster(ActivityMainItem.key)">

                                                        <div data-options="dxTemplate: { name : 'ActivityRequestDetailsGridContent' }">

                                                            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">

                                                                @*<div class="master-detail-caption-st"></div>*@
                                                                <div dx-data-grid="ActivityRequestDetailsGrid(ActivityMainItem.key)"></div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>